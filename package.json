{
  "name": "obraaldia-refactoring-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "compodoc:build": "compodoc -p tsconfig.doc.json",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s",
    "compodoc:serve": "compodoc -s",
    "compodoc": "npx compodoc -p tsconfig.doc.json",
    "lint": "ng lint",
    "prepare": "husky install",
    "build:production": "ng build --prod && ng run obraaldia-refactoring-frontend:server:production",
    "build:dev": "ng build -c=development --base-href",
    "build:qa": "ng build -c=qa --base-href / && ng run obraaldia-refactoring-frontend:server:qa",
    "build:stage": "ng build -c=stage --base-href / && ng run obraaldia-refactoring-frontend:server:stage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "primeicons": "^5.0.0",
    "primeng": "^13.1.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.4",
    "@angular-eslint/builder": "13.5.0",
    "@angular-eslint/eslint-plugin": "13.5.0",
    "@angular-eslint/eslint-plugin-template": "13.5.0",
    "@angular-eslint/schematics": "13.5.0",
    "@angular-eslint/template-parser": "13.5.0",
    "@angular/cli": "~13.1.4",
    "@angular/compiler-cli": "~13.1.0",
    "@compodoc/compodoc": "^1.1.21",
    "@types/jasmine": "~3.10.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine-core": "~3.10.0",
    "jest": "^27.4.7",
    "jest-preset-angular": "^11.1.0",
    "prettier": "^2.8.8",
    "typescript": "~4.5.2",
    "husky": "^8.0.0"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.ts"
    ],
    "globalSetup": "jest-preset-angular/global-setup"
  }
}
